# Copy this file to .env and fill in your values

# Database (for PostgresSaver checkpointer)
POSTGRES_USER=teammate
POSTGRES_PASSWORD=teammate_secret
POSTGRES_DB=teammate_memory
DATABASE_URL=postgresql://teammate:teammate_secret@db:5432/teammate_memory

# Redis (for task queueing)
REDIS_URL=redis://redis:6379

# LLM Providers
ANTHROPIC_API_KEY=sk-ant-api03-...
DEEPSEEK_API_KEY=sk-...
OPENAI_API_KEY=sk-...

# Task Budget (for cost control gatekeeper)
# Maximum tokens/cost allowed per task - set to reasonable default
MAX_TASK_BUDGET=10000
